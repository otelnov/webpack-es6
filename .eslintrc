{
  "env": {
    "browser": 2,
    "node": 2,
    "mocha": 2
  },
  "globals": {
    "io": 2,
    "google": 2,
    "device": 2,
    "_": 2,
    "$": 2,
    "angular": 2,
    "jQuery": 2
  },
  "rules": {
    "indent": [2, 2],
    // Possible Errors
    "no-comma-dangle": 2,
    // except-parens, always
    "no-cond-assign": "always",
    // todo: apply
    // "no-console": 1,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-empty-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    // todo: apply no-extra-parens
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    // todo: apply valid-jsdoc
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    // Best Practices
    "block-scoped-var": 2,
    // "complexity": 5,
    "consistent-return": 2,
    "curly": 2,
    "default-case": 2,
    // [2, {"allowKeywords": true}]
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-empty-label": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-process-env": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-void": 2,
    // todo: apply no-warning-comments
    // "no-warning-comments": [1, { "terms": ["todo", "fixme", "any other term"], "location": "anywhere" }],
    "no-with": 2,
    "radix": 2,
    "wrap-iife": [
      2,
      "inside"
    ],
    "yoda": "never",
    "strict": false,
    "no-extra-strict": 2,
    // Variables
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    // todo: apply no-shadow
    "no-shadow": 0,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": "nofunc",
    // Node.js
    // todo: apply handle-callback-err
    "handle-callback-err": [
      1,
      "^.*(e|E)rr"
    ],
    "no-mixed-requires": [
      1,
      2
    ],
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-sync": 2,
    // Stylistic Issues
    "brace-style": [
      2,
      "1tbs",
      {"allowSingleLine": true}
    ],
    "camelcase": 2,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "consistent-this": [
      1,
      "self"
    ],
    // not sure
    "eol-last": 0,
    // todo: apply func-names
    "func-names": 0,
    "func-style": [
      2,
      "declaration"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    // todo: apply max-nested-callbacks
    "max-nested-callbacks": [
      0,
      3
    ],
    // todo: remove S === requrie('string');
    "new-cap": [
      2,
      {
        "newIsCap": true,
        "capIsNew": true,
        "capIsNewExceptions": [
          "S",
          "ObjectId",
          "Object",
          "Function",
          "Number",
          "String",
          "Boolean",
          "Date",
          "Array",
          "Symbol",
          "RegExp"
        ]
      }
    ],
    "new-parens": 2,
    "no-array-constructor": 2,
    // todo: apply no-inline-comments
    "no-inline-comments": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [
      2,
      {"max": 1}
    ],
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-space-before-semi": 2,
    "no-spaced-func": 2,
    "no-underscore-dangle": false,
    "no-trailing-spaces": 2,
    "no-wrap-func": 2,
    "operator-assignment": [
      1,
      "always"
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    // not sure
    "quote-props": [
      2,
      "as-needed"
    ],
    // not sure
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "semi": [
      2,
      "always"
    ],
    // "sort-vars":  [1, { "ignoreCase": true }],
    // todo: apply space-after-function-name
    "space-after-function-name": [
      2,
      "never"
    ],
    "space-after-keywords": [
      2,
      "always",
      {"checkFunctionKeyword": true}
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    // todo: apply space-in-brackets
    "space-in-brackets": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-return-throw-case": 2,
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    // Legacy
    "max-len": [
      1,
      120,
      4
    ],
    // todo: apply max-params
    "max-params": [
      0,
      3
    ],
    // todo: apply max-statements
    "max-statements": [
      1,
      20
    ],
    "no-bitwise": 2
  }
}
